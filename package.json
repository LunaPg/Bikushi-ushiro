{
  "dependencies": {
    "@types/node-pushnotifications": "^1.0.3",
    "@types/uuid": "^3.4.7",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-typescript": "^0.14.0",
    "gbfs-client": "^v0.8.1",
    "install": "^0.13.0",
    "node-pushnotifications": "^1.2.1",
    "npm": "^6.14.6",
    "typescript": "^3.8.2",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.9",
    "@types/faker": "^4.1.9",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "@types/sinon": "^7.0.13",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.16.4",
    "sinon": "^7.4.1",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.6.2"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "sourceMap": true,
    "instrument": true,
    "exclude": [
      "**/tests/**",
      "coverage"
    ]
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --fix --ext .ts src ",
    "test": "mocha --opts ./src/mocha.opts './src/tests/**/*spec.ts'",
    "test:debug": "node --inspect-brk mocha -r  --all ts-node/register  './src/tests/**/*spec.ts'",
    "test:cov": "nyc mocha --opts ./src/mocha.opts './src/tests/**/*spec.ts'",
    "test:cov:file": "nyc  --reporter=lcov mocha --opts ./src/mocha.opts './src/tests/**/*spec.ts'",
    "test:watch": "mocha -r ts-node/register  --watch './src/tests/**/*spec.ts'",
    "start": "ts-node src/index.ts"
  }
}
