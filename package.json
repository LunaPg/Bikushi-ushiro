{
  "dependencies": {
    "@types/node-pushnotifications": "^1.0.3",
    "@types/uuid": "^3.4.7",
    "gbfs-client": "^0.7.4",
    "node-pushnotifications": "^1.2.0",
    "typescript": "^3.7.5",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.8",
    "@types/faker": "^4.1.9",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "@types/sinon": "^7.0.13",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "faker": "^4.1.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sinon": "^7.4.1",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.6.1"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "sourceMap": true,
    "instrument": true,
    "exclude": [
      "**/tests/**"
    ]
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --fix --ext .ts",
    "test": "mocha --opts ./src/mocha.opts './src/tests/**/*spec.ts'",
    "test:debug": "node --inspect-brk mocha -r  --all ts-node/register  './src/tests/**/*spec.ts'",
    "test:cov": "nyc mocha --opts ./src/mocha.opts './src/tests/**/*spec.ts'",
    "test:cov:file": "nyc  --reporter=lcov mocha --opts ./src/mocha.opts './src/tests/**/*spec.ts'",
    "test:watch": "mocha -r ts-node/register  --watch './src/tests/**/*spec.ts'",
    "start": "ts-node src/index.ts"
  }
}
